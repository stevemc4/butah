{
  "name": "butah",
  "version": "1.0.0",
  "description": "Common TypeScript project scaffolding",
  "main": "dist/app.js",
  "scripts": {
    "test": "eslint src/**/*.ts --fix",
    "build": "tsc",
    "dev": "ts-node --project tsconfig.json src/app.ts",
    "start": "node dist/app.js",
    "watch": "NODE_ENV=development nodemon -e ts --watch ./src --exec \"npm run dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevemc4/butah.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevemc4/butah/issues"
  },
  "homepage": "https://github.com/stevemc4/butah#readme",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  }
}
